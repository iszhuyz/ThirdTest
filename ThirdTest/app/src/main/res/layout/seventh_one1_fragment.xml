<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/photo_7111"
        style="@style/OnePhoto"
        android:layout_marginTop="8dp"
        />
    <TextView
        android:id="@+id/text_7111"
        style="@style/OneStyle"
        android:text="一键清理"
        />
    <TextView
        android:id="@+id/text_7112"
        style="@style/FourStyle"
        android:text="可清理垃圾："
        android:paddingLeft="50dp"
        android:layout_below="@+id/photo_7111"
        android:layout_marginTop="10dp"
        />
    <Button
        android:id="@+id/button_7111"
        style="@style/Button"
        android:text="一键清理"
        android:layout_below="@+id/text_7111"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="500dp"
        android:nextFocusLeft="@+id/button_7111"
        android:nextFocusUp="@+id/button_7111"
        android:nextFocusDown="@+id/button_7111"
        />
    <requestFocus/>

</RelativeLayout>
